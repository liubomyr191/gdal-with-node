{
  "name": "gdal",
  "version": "0.0.1",
  "description": "Native GDAL bindings",
  "url": "http://github.com/naturalatlas/node-gdal",
  "main": "./lib/gdal.js",
  "keywords": [
    "gis",
    "geo",
    "geography",
    "geospatial",
    "raster",
    "gdal",
    "libgdal"
  ],
  "binary": {
    "module_name": "gdal",
    "module_path": "./lib/binding/",
    "remote_path": "v{version}",
    "host": "https://node-gdal.s3-us-west-1.amazonaws.com",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/naturalatlas/node-gdal.git"
  },
  "author": {
    "name": "Brandon Reavis",
    "url": "https://github.com/brandonreavis",
    "email": "brandon@naturalatlas.com"
  },
  "licenses": [
    {
      "type": "Apache License, Version 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "pretest": "npm install --dev",
    "test": "mocha test -R list",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "dependencies": {
    "node-pre-gyp": "~0.5.13"
  },
  "devDependencies": {
    "mocha": "~1.x",
    "chai": "~1.9.1"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "engines": {
    "node": ">=0.8.0"
  }
}
